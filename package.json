{
  "name": "redux-filterlist",
  "version": "0.2.6",
  "description": "A Higher Order Component using react-redux for creating lists with filters, sotring, paginatinon, endless scroll etc.",
  "main": "./dist/redux-filterlist.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "build:lib": "babel src --out-dir lib --ignore __tests__,test-utils",
    "build:umd": "cross-env NODE_ENV=development webpack",
    "build:umd:min": "cross-env NODE_ENV=production webpack",
    "lint": "eslint src",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest",
    "test:unit:watch": "jest --watchAll",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtaits/redux-filterlist.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "redux-filterlist",
    "filterlist",
    "filter",
    "sort",
    "pagination",
    "table"
  ],
  "author": "Vadim Taits",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vtaits/redux-filterlist/issues"
  },
  "homepage": "https://github.com/vtaits/redux-filterlist#readme",
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-redux": "^4.3.0 || ^5.0.0",
    "redux": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "check-prop-types": "^1.1.1",
    "cross-env": "^5.0.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "flux-standard-action": "^1.2.0",
    "jest": "^22.0.4",
    "path": "^0.12.7",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "sinon": "^4.0.1",
    "webpack": "^3.7.1"
  },
  "dependencies": {
    "array-insert": "^2.0.0",
    "es6-error": "^4.0.2",
    "lodash.isequal": "^4.5.0",
    "prop-types": "^15.6.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/examples/"
    ],
    "setupFiles": [
      "./setup-jest.js"
    ]
  }
}
